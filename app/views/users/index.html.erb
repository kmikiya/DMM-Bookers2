<div>
<h2>Users</h2>

<table>
  <thead>
    <tr>
        <td>image</td>
        <td>name</td>
    </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= attachment_image_tag user, :profile_image,  :fill, 60, 60, fallback: "no_image.jpg" %></td> <!--blogモデル内のtitleカラムという意味-->
      <td><%= user.name %></td>
      <td></td>
      <td>
        <%= link_to "Show", user_path(user.id) %>
      </td>
<% end %>
    </tr>
  </tbody>
</table>
</div>
<%= render 'books/list', book: @book, user: @user %>